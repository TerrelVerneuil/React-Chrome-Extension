{
  
  "manifest_version": 2,
  "name": "React-Chrome-Extension",
  "version": "1.0",
  "permissions": ["idle","tabs", "storage", "<all_urls>", "activeTab","contextMenus"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}